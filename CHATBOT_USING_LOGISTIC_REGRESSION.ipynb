        "\n",
        "  for word in sentence_words:\n",
        "        if word in bag_of_words:\n",
        "            bag_vector[bag_of_words.index(word)] += 1\n",
        "\n",
        "  z = np.dot(bag_vector,weights) + biases\n",
        "  sigmoid = 1 /(1+ np.exp(-z))\n",
        "  tag_index = np.argmax(sigmoid)\n",
        "  tag = tags[tag_index]\n",
        "  print(tag)\n",
        "  return tag\n",
        "\n"
